  SUBPARTITION s86 ENGINE = InnoDB,
  SUBPARTITION s87 ENGINE = InnoDB,
  SUBPARTITION s88 ENGINE = InnoDB));
  
  
  
insert into cellinfo (SELECT lac,ci,lat,lon,acc,date,validity,addr,province,
city,district,township,b_lng,b_lat,g_lng,g_lat from cellinfo_v2 where lac>=0 and lac<1000 and ci<=65535);
