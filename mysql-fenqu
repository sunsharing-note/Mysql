  SUBPARTITION s77 ENGINE = InnoDB,
  SUBPARTITION s78 ENGINE = InnoDB,
  SUBPARTITION s79 ENGINE = InnoDB),
 PARTITION p9 VALUES LESS THAN MAXVALUE
 (SUBPARTITION s89 ENGINE = InnoDB,
  SUBPARTITION s81 ENGINE = InnoDB,
  SUBPARTITION s82 ENGINE = InnoDB,
  SUBPARTITION s83 ENGINE = InnoDB,
  SUBPARTITION s84 ENGINE = InnoDB,
