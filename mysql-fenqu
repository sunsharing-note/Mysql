CREATE TABLE `cellinfo` (
  `lac` int(11) NOT NULL,
  `ci` int(11) NOT NULL,
  `lat` float(9,6) DEFAULT NULL,
  `lon` float(9,6) DEFAULT NULL,
  `acc` int(11) DEFAULT NULL,
  `date` int(11) DEFAULT NULL,
  `validity` float(3,2) DEFAULT NULL,
  `addr` varchar(256) DEFAULT NULL,
  `province` varchar(64) DEFAULT NULL,
  `city` varchar(64) DEFAULT NULL,
  `district` varchar(64) DEFAULT NULL,
  `township` varchar(64) DEFAULT NULL,
  `b_lng` float(9,6) DEFAULT NULL,
  `b_lat` float(9,6) DEFAULT NULL,
  `g_lng` float(9,6) DEFAULT NULL,
  `g_lat` float(9,6) DEFAULT NULL,
  PRIMARY KEY (`lac`,`ci`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
PARTITION BY RANGE (lac)
SUBPARTITION BY HASH (ci)
(PARTITION p0 VALUES LESS THAN (4600)
 (SUBPARTITION s0 ENGINE = InnoDB,
  SUBPARTITION s1 ENGINE = InnoDB,
  SUBPARTITION s2 ENGINE = InnoDB,
  SUBPARTITION s3 ENGINE = InnoDB,
  SUBPARTITION s4 ENGINE = InnoDB,
  SUBPARTITION s5 ENGINE = InnoDB,
  SUBPARTITION s6 ENGINE = InnoDB,
  SUBPARTITION s7 ENGINE = InnoDB,
  SUBPARTITION s8 ENGINE = InnoDB),
 PARTITION p1 VALUES LESS THAN (9200)
 (SUBPARTITION s9 ENGINE = InnoDB,
  SUBPARTITION s10 ENGINE = InnoDB,
  SUBPARTITION s11 ENGINE = InnoDB,
  SUBPARTITION s12 ENGINE = InnoDB,
  SUBPARTITION s13 ENGINE = InnoDB,
  SUBPARTITION s14 ENGINE = InnoDB,
  SUBPARTITION s15 ENGINE = InnoDB,
  SUBPARTITION s16 ENGINE = InnoDB,
  SUBPARTITION s17 ENGINE = InnoDB),
 PARTITION p2 VALUES LESS THAN (13800)
 (SUBPARTITION s26 ENGINE = InnoDB,
  SUBPARTITION s18 ENGINE = InnoDB,
  SUBPARTITION s19 ENGINE = InnoDB,
  SUBPARTITION s20 ENGINE = InnoDB,
  SUBPARTITION s21 ENGINE = InnoDB,
  SUBPARTITION s22 ENGINE = InnoDB,
  SUBPARTITION s23 ENGINE = InnoDB,
  SUBPARTITION s24 ENGINE = InnoDB,
  SUBPARTITION s25 ENGINE = InnoDB),
